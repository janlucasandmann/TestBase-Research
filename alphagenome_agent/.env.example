# TestBase Research AlphaGenome Agent Configuration
# Copy this file to .env and fill in your actual values

# AlphaGenome API Configuration
# Get your API key from AlphaGenome platform
ALPHAGENOME_API_KEY=your_api_key_here

# AlphaGenome API base URL (usually doesn't need to be changed)
ALPHAGENOME_BASE_URL=https://alphagenome.googleapis.com/v1

# Development and Testing Settings
# Set to 1 to use mock data instead of real API calls (useful for development/testing)
USE_MOCK=0

# Logging configuration
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Optional: Override default directories (relative to project root)
# DATA_DIR=data
# OUTPUT_DIR=data/outputs
# FIGURES_DIR=data/outputs/figures

# Optional: API retry configuration
# RETRY_MAX_ATTEMPTS=3
# RETRY_WAIT_SECONDS=1.0
# RETRY_MAX_WAIT=10.0

# Optional: Analysis parameters
# DEFAULT_WINDOW_SIZE=25000
# MAX_INTERVAL_SIZE=1000000